<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#F3F4F6" />
            <stroke android:width="1dp" android:color="@color/btn_secondary_border_hover" />
            <corners android:radius="16dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/btn_secondary" />
            <stroke android:width="1dp" android:color="@color/btn_secondary_border" />
            <corners android:radius="16dp" />
        </shape>
    </item>
</selector>

